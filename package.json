{
  "name": "twitcherbot",
  "version": "1.0.0",
  "description": "Powerful all-in-one bot for Twitch streamers.",
  "main": "main.js",
  "scripts": {
    "scss": "grunt scss",
    "start": "grunt scss && grunt include-source && electron --inspect=5858 . --dev --js-flags=\"--harmony\"",
    "start:noscss": "electron . --js-flags=\"--harmony\"",
    "rebuild": "node-gyp rebuild --directory=node_modules/robotjs/ --runtime=electron --target=7.2.4 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "rebuild:new": "npm rebuild --runtime=electron --target=7.2.4 --disturl=https://atom.io/download/atom-shell --abi=83",
    "rebuild:darwin": "node-gyp rebuild --directory=node_modules/robotjs/ --runtime=electron --target=7.2.4 --arch=darwin --dist-url=https://atom.io/download/atom-shell",
    "setup": "npm install && npm rebuild && npm run rebuild"
  },
  "repository": "https://github.com/crowbartools/Twitchbot",
  "keywords": [
    "twitch",
    "twitch.tv",
    "stream",
    "streaming",
    "electron"
  ],
  "author": "The Streaming Community",
  "license": "GPL-3.0",
